{
  "scripts": {
    "start": "concurrently 'nodemon --watch webpack.config.js --exec webpack serve --env mode=development' 'npm run generate'",
    "build": "webpack --env mode=production",
    "serve": "npm run build && npx http-server -c-1 ./dist",
    "generate": "graphql-codegen --config src/generated/codegen.ts --watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@types/node": "^18.14.0",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@apollo/client": "^3.7.8",
    "bootstrap": "^5.2.3",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
